<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.book.BooksController">
   <children>
      <VBox layoutX="225.0" layoutY="50.0" prefHeight="300.0" prefWidth="150.0">
         <children>
            <Button mnemonicParsing="false" onAction="#switchToAddBooks" prefHeight="100.0" prefWidth="150.0" text="Add book" />
            <Button mnemonicParsing="false" onAction="#switchToList" prefHeight="100.0" prefWidth="150.0" text="List of all books" />
            <Button mnemonicParsing="false" onAction="#switchToPrev" prefHeight="100.0" prefWidth="150.0" text="Back" />
         </children>
      </VBox>
   </children>
</AnchorPane>
